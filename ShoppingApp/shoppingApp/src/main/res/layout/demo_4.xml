<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">




    <LinearLayout
        android:id="@+id/llMainTypeFour"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/clrBgPink2">

        <RelativeLayout
            android:id="@+id/rlHeaderFour"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <com.util.CircularImageView
                android:id="@+id/ivLastRelatedSearchTagCircularImage"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginLeft="10sp"
                android:layout_marginTop="10sp"
                android:src="@drawable/banner1"
                android:background="@drawable/img_user_shadow"
                android:layout_alignParentLeft="true"/>

            <TextView
                android:id="@+id/tvLastRelatedSearchTag"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/ivLastRelatedSearchTagCircularImage"
                android:layout_toLeftOf="@+id/rlViewAllTagFour"
                android:maxLines="1"
                android:paddingBottom="10sp"
                android:paddingLeft="10sp"
                android:paddingRight="10sp"
                android:paddingTop="10sp"
                android:text="Last Related Search"
                android:textColor="@color/clrBtnPinkDark"
                android:textSize="@dimen/text_20s"/>

            <TextView
                android:id="@+id/rlViewAllTagFour"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/pink_btn_round_rect"
                android:layout_marginRight="10dp"
                android:paddingBottom="5sp"
                android:paddingLeft="10sp"
                android:paddingRight="10sp"
                android:paddingTop="5sp"
                android:text="View All"
                android:textColor="@color/clrWhite"
                android:textSize="@dimen/text_12s"/>
        </RelativeLayout>

        <!--<View-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="5dp"-->
        <!--android:layout_marginTop="5dp"-->
        <!--android:background="@drawable/elevation_bg"/>-->


        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/cardViewLastRelated"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardUseCompatPadding="true"
            android:layout_gravity="center"
            card_view:cardCornerRadius="9sp"
            card_view:cardElevation="9sp">
            <!--android:layout_marginLeft="15sp"
            android:layout_marginRight="15sp"
            android:layout_marginBottom="10sp"
            android:layout_marginTop="5sp"-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="300sp"

                android:gravity="center"

                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_margin="10sp"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivLastRelatedSearchProductImgOne"
                        android:layout_width="match_parent"
                        android:layout_height="245sp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/one"/>

                    <TextView
                        android:id="@+id/tvLastRelatedSearchProductNameOne"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="New Arrivals"
                        android:textColor="@color/clrBtnPink"
                        android:textSize="@dimen/text_16s"/>

                    <TextView
                        android:id="@+id/tvLastRelatedSearchProductPriceOne"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="Rs. 1500/-"
                        android:textColor="@color/clrBtnPink"
                        android:textSize="@dimen/text_12s"/>
                </LinearLayout>

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/clrBtnPink"
                    />


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:layout_margin="10sp"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/ivLastRelatedSearchProductImgTwo"
                            android:layout_width="match_parent"
                            android:layout_height="90dp"
                            android:layout_centerVertical="true"
                            android:src="@drawable/one"/>

                        <TextView
                            android:id="@+id/tvLastRelatedSearchProductNameTwo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:maxLines="1"
                            android:text="New Arrivals"
                            android:textColor="@color/clrBtnPink"
                            android:textSize="@dimen/text_16s"/>

                        <TextView
                            android:id="@+id/tvLastRelatedSearchProductPriceTwo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:maxLines="1"
                            android:text="Rs. 1500/-"
                            android:textColor="@color/clrBtnPink"
                            android:textSize="@dimen/text_12s"/>
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/clrBtnPink"
                        />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:layout_margin="10sp"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/ivLastRelatedSearchProductImgThree"
                            android:layout_width="match_parent"
                            android:layout_height="90sp"
                            android:layout_centerVertical="true"
                            android:src="@drawable/one"/>

                        <TextView
                            android:id="@+id/tvLastRelatedSearchProductNameThree"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:maxLines="1"
                            android:text="New Arrivals"
                            android:textColor="@color/clrBtnPink"
                            android:textSize="@dimen/text_16s"/>

                        <TextView
                            android:id="@+id/tvLastRelatedSearchProductPriceThree"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:maxLines="1"
                            android:text="Rs. 1500/-"
                            android:textColor="@color/clrBtnPink"
                            android:textSize="@dimen/text_12s"/>
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </android.support.v7.widget.CardView>



    </LinearLayout>












